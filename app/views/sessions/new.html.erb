<% provide(:title, "Login") %>
<h1>Login</h1>

<div>
    <%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :email, "Email" %>
    <%= f.email_field :email %>

    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>

    <%= f.submit "Submit"%>

    <% end %>
</div>

<div>
    <p>New user?</p>
</div>

